}

class CostureroSanJuan{}
class CostureroJohnston{}
class Uniforme{
prendaSuperior
prendaInferior
calzado
fabricar(costurero)
}

enum Categoria{
}

enum TipoDeMaterial{}
enum Trama{}

class Tipo{
}


Atuendo -->"(2,4)" Prenda 
Tipo --> Prenda
Categoria --> Tipo
Prenda --> Material
